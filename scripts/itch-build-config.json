      { "from": "assets/characters/axolotl", "to": "assets/axolotl" },
      { "from": "assets/items/unko_32.png", "to": "assets/items/unko_32.png" }
    ],
    "pathRewrites": [
      ["href=\"/styles.css\"", "href=\"./styles.css\""],
