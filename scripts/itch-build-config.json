      ["'/assets/characters/axolotl/'", "'./assets/axolotl/'"],
      ["/assets/items/unko_32.png", "./assets/items/unko_32.png"]
    ],
    "rewriteFiles": ["index.html", "axolotl-shop.js", "mascot.js"],
    "useHeaderStub": true
  },
  "mine-tap": {
    "type": "plain",
    "gameFiles": [],
    "copyDirs": ["src", "locales"],
    "assets": [
      { "from": "assets/icon", "to": "assets/icon" },
      { "from": "assets/ores", "to": "assets/ores" },
      { "from": "assets/money", "to": "assets/money" },
      { "from": "assets/items/pickaxe", "to": "assets/items/pickaxe" },
      { "from": "assets/sound/damege", "to": "assets/sound/damege" },
      { "from": "assets/characters", "to": "assets/characters" }
    ],
    "pathRewrites": [
      ["/assets/", "./assets/", "regex"]
    ],
    "rewriteFiles": ["index.html"],
    "useHeaderStub": false
  }
